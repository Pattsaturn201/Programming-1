/*We have a matrix sum of  A and B matrices
*/

#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char const *argv[])
{
    int n, m;
    printf("Enter a value for the number of rows:\n");
    scanf("%d",&n);
    printf("Enter a value for the number of columns:\n");
    scanf("%d",&m);

    int matrixA[n][m];
    int matrixB[n][m];
    int matrixCtotal[n][m];

    return 0;
}
